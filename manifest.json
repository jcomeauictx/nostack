{
  "name": "Nostack: the substack eliminator",
  "version": "0.0.0.3",
  "description": "Replaces all substack links with cdc.gov",
  "content_scripts": [
    {
      "matches": ["https://*.substack.com/*"],
      "js": ["nostack.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
